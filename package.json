{
  "name": "matheus-dev-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:ci": "yarn lint -- --quiet",
    "format": "prettier \"(scripts|src)/**/**.(js|ts|tsx)\" --write",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "test:watch": "yarn test -- --watch",
    "test:ci": "yarn test -- --ci",
    "pkg:upgrade": "taze"
  },
  "browserslist": [
    "chrome >= 90",
    "edge >= 90",
    "firefox >= 90",
    "opera >= 75",
    "safari >= 13",
    "not IE 11",
    "not dead"
  ],
  "dependencies": {
    "@vanilla-extract/css": "^1.9.5",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "next": "^13.2.1",
    "next-intl": "2.11.0-beta.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "^2.8.4",
    "taze": "^0.8.5",
    "typescript": "^4.9.5"
  }
}
