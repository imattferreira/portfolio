{
  "name": "matheus-dev-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier \"(scripts|src)/**/**.(js|ts|tsx)\" --write",
    "lint": "next lint",
    "lint:ci": "yarn lint -- --quiet",
    "pkg:upgrade": "taze",
    "start": "next start",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "test:watch": "yarn test -- --watch",
    "test:ci": "yarn test -- --ci",
    "test:a11y": "pa11y-ci"
  },
  "browserslist": [
    "chrome >= 90",
    "edge >= 90",
    "firefox >= 90",
    "opera >= 75",
    "safari >= 13",
    "not IE 11",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/sprinkles": "^1.5.2",
    "next": "^13.3.0",
    "next-intl": "2.13.0-beta.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "pa11y-ci": "^3.0.1",
    "prettier": "^2.8.7",
    "taze": "^0.9.1",
    "typescript": "^5.0.4"
  }
}
